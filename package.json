{
  "name": "rsgen",
  "version": "0.0.9",
  "description": "Resources generation for Typeplate",
  "main": "./bin/rsgen.js",
  "bin": {
    "rsgen": "./bin/rsgen.js"
  },
  "directories": {
    "lib": "lib"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": false,
    "commitLimit": false
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "version": "git add package.json && git add README.md && auto-changelog -p && git add CHANGELOG.md && git commit -m \"Update changelog\" --no-verify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konfer-be/rsgen.git"
  },
  "author": "info@konfer.be",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/konfer-be/rsgen/issues"
  },
  "homepage": "https://github.com/konfer-be/rsgen#readme",
  "_moduleAliases": {
    "@commands": "bin/commands",
    "@interfaces": "bin/types/interfaces",
    "@utils": "bin/utils"
  },
  "dependencies": {
    "camelcase": "6.2.0",
    "execa": "5.0.0",
    "inquirer": "^8.0.0",
    "listr": "^0.14.3",
    "module-alias": "^2.2.1",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "14.14.35",
    "auto-changelog": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
